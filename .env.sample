# ---- App profile / mode ----
APP_PROFILE=DEV
TELEGRAM_USE_POLLING=true

# ---- Telegram bot ----
BOT_USERNAME=YourBotName
TELEGRAM_BOT_TOKEN=__PUT_REAL_TOKEN_HERE__
OWNER_TELEGRAM_ID=0

# ---- DB ----
DATABASE_URL=jdbc:postgresql://postgres:5432/botdb
DATABASE_USER=botuser
DATABASE_PASSWORD=botpass

# ---- Health ----
HEALTH_DB_TIMEOUT_MS=150

# ---- Flyway: фиксируем только PostgreSQL-миграции ----
FLYWAY_LOCATIONS=classpath:db/migration/postgresql

